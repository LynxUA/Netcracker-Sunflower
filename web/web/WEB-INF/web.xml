    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenerateServicesServlet</servlet-name>
        <url-pattern>/generateservices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveLocationServlet</servlet-name>
        <url-pattern>/savelocation</url-pattern>
