        <servlet-name>SOInfoServlet</servlet-name>
        <servlet-class>com.sunflower.web.SOInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SIInfoServlet</servlet-name>
        <servlet-class>com.sunflower.web.SIInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInfoServlet</servlet-name>
