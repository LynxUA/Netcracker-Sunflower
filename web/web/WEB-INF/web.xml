    <ejb-local-ref>
        <ejb-ref-name>ejb/User</ejb-ref-name>
        <ejb-ref-type>Entity</ejb-ref-type>
        <local-home>com.sunflower.ejb.user.LocalUserHome</local-home>
        <local>com.sunflower.ejb.user.LocalUser</local>
